{
  "@context": "https://schema.org",
  "@type": "WebPage",
  "name": "{gameTitle} - Free Online Game",
  "description": "Play {gameTitle} online for free. {gameDescription}",
  "url": "https://italianbrainrot-games.github.io/game/{gameSlug}",
  "isPartOf": {
    "@type": "WebSite",
    "name": "Italian Brainrot Games",
    "url": "https://italianbrainrot-games.github.io"
  },
  "breadcrumb": {
    "@type": "BreadcrumbList",
    "itemListElement": [
      {
        "@type": "ListItem",
        "position": 1,
        "name": "Home",
        "item": "https://italianbrainrot-games.github.io"
      },
      {
        "@type": "ListItem",
        "position": 2,
        "name": "Games",
        "item": "https://italianbrainrot-games.github.io/games"
      },
      {
        "@type": "ListItem",
        "position": 3,
        "name": "{categoryName}",
        "item": "https://italianbrainrot-games.github.io/category/{categorySlug}"
      },
      {
        "@type": "ListItem",
        "position": 4,
        "name": "{gameTitle}",
        "item": "https://italianbrainrot-games.github.io/game/{gameSlug}"
      }
    ]
  },
  "mainEntity": {
    "@type": "Game",
    "name": "{gameTitle}",
    "description": "{gameDescription}",
    "genre": "{gameCategory}",
    "gamePlatform": "Web Browser",
    "applicationCategory": "Game",
    "operatingSystem": "Any",
    "offers": {
      "@type": "Offer",
      "price": "0",
      "priceCurrency": "USD",
      "availability": "https://schema.org/InStock"
    }
  }
} 